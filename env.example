# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Razorpay Configuration
NEXT_PUBLIC_RAZORPAY_KEY_ID=rzp_test_your_key_id
RAZORPAY_KEY_SECRET=your_razorpay_secret_key
RAZORPAY_WEBHOOK_SECRET=your_webhook_secret

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Cash Karma

# Database Configuration (for migrations)
DATABASE_URL=postgresql://postgres:password@localhost:5432/postgres

# Edge Functions Configuration
SUPABASE_FUNCTIONS_URL=https://your-project.supabase.co/functions/v1

# Optional: Analytics and Monitoring
NEXT_PUBLIC_ANALYTICS_ID=your-analytics-id
SENTRY_DSN=your-sentry-dsn 

# Environment variables for development

# i18next Configuration
VITE_I18N_DEBUG=false
VITE_I18N_LOG_LEVEL=warn

# Console Logging Control
VITE_ENABLE_CONSOLE_FILTERING=true
VITE_SUPPRESS_INFO_LOGS=true
VITE_SUPPRESS_DEBUG_LOGS=true

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_LOG_PERFORMANCE_METRICS=true

# Development Tools
VITE_SHOW_ALL_LOGS=false
VITE_FILTER_SPECIFIC_MESSAGES=true 